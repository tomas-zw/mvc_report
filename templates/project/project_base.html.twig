{# templates/project/project_base.html.twig #}
<!DOCTYPE html>
<html>
    <head>
        <meta charset="UTF-8">
        <title>{% block title %}My Project{% endblock %}</title>
        {% block stylesheets %}
            <link rel="stylesheet" type="text/css" href="{{ asset('css/base.css') }}"/>
        {% endblock %}
    </head>
    <body>

        {{ include('project/header.html.twig')}}
        {{ include('project/navbar.html.twig')}}

        {% block body %}
            <div id="content">
                {% block content %} Project {% endblock %}
            </div>
        {% endblock %}

        {{ include('project/footer.html.twig')}}

    </body>
</html>
